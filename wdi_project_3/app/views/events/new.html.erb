<h1>Create your event here</h1>


<%= form_for :events, url: "/events" do |f| %>
  TITLE: <%= text_field_tag :title %><br>
  <%= text_field_tag :date %>
  <%= text_field_tag :start_time %>
  <%= text_field_tag :end_time %>
  <%= text_field_tag :content %>
  <%= text_field_tag :topic %>
  <%= text_field_tag :price %>
  <%= text_field_tag :type_of_event %>
  <%= text_field_tag :user_id, current_user.id %><br>
  <%= f.submit "Create" %>
<% end %>
